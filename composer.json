{
    "name": "portabilis/i-educar",
    "description": "Software livre de gest√£o escolar",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "version": "2.7.2",
    "keywords": [
        "Portabilis",
        "i-Educar"
    ],
    "homepage": "https://github.com/portabilis/i-educar",
    "require": {
        "php": "^8.0|^8.1",
        "ext-gd": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "ext-pgsql": "*",
        "aws/aws-sdk-php-laravel": "^3.7",
        "composer/semver": "^3.2",
        "cossou/jasperphp": "^2.8",
        "dex/composer-plug-and-play": "^0.8.0",
        "dex/frontier": "^0.2.0",
        "doctrine/dbal": "^2.13",
        "fruitcake/laravel-cors": "^2.0",
        "google/recaptcha": "^1.2",
        "guzzlehttp/guzzle": "^7.3",
        "honeybadger-io/honeybadger-laravel": "^3.12",
        "intervention/image": "^2.6",
        "laravel/framework": "^9.00",
        "laravel/horizon": "^v6.7.9",
        "laravel/sanctum": "^2.14.1",
        "laravel/tinker": "^2.6",
        "laravel/ui": "^3.4",
        "league/csv": "^9.7",
        "league/flysystem-aws-s3-v3": "^3.0",
        "league/flysystem-ftp": "^3.0",
        "league/flysystem-sftp-v3": "^3.0",
        "maatwebsite/excel": "^3.1",
        "mll-lab/graphql-php-scalars": "^5.4",
        "nuwave/lighthouse": "^5",
        "phpoffice/phpspreadsheet": "^1.8",
        "predis/predis": "^1.1",
        "symfony/http-client": "^6.0",
        "symfony/mailgun-mailer": "^6.0"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.7",
        "brianium/paratest": "^6.3",
        "fakerphp/faker": "^1.15",
        "filp/whoops": "^2.14",
        "friendsofphp/php-cs-fixer": "^3.2",
        "laravel/sail": "^0.0.5",
        "laravel/telescope": "^4.7",
        "mockery/mockery": "^1.4.3",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.7",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "classmap": [
            "ieducar/intranet/include",
            "ieducar/lib",
            "ieducar/modules/*/Model",
            "ieducar/modules/Avaliacao/Service",
            "ieducar/modules/Avaliacao/Fixups",
            "ieducar/modules/Avaliacao/Views",
            "ieducar/modules/FormulaMedia/Validate"
        ],
        "files": [
            "ieducar/intranet/include/funcoes.inc.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Custom\\": "database/custom/",
            "Database\\Seeders\\": "database/seeders/",
            "iEducar\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "classmap": [
            "tests/Unit"
        ]
    },
    "include-path": [
        "ieducar",
        "ieducar/intranet",
        "ieducar/lib",
        "ieducar/modules"
    ],
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "scripts": {
        "check-style": "vendor/bin/php-cs-fixer --diff --dry-run --verbose fix",
        "fix-style": "vendor/bin/php-cs-fixer fix",
        "set-permissions": "find bootstrap/cache -type d -exec chmod 777 {} + && find storage -type d -exec chmod 777 {} +",
        "npm": "yarn install",
        "new-install": [
            "@composer install",
            "@composer plug-and-play:install",
            "@set-permissions",
            "@php artisan key:generate --ansi",
            "@php artisan legacy:link --ansi",
            "@php artisan migrate --seed --force --ansi",
            "@npm"
        ],
        "update-install": [
            "@composer update",
            "@composer plug-and-play:update",
            "@set-permissions",
            "@php artisan legacy:link --ansi",
            "@php artisan migrate --force --ansi",
            "@npm"
        ],
        "post-install-cmd": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "dex/composer-plug-and-play": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
